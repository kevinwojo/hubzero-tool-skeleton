<?xml version="1.0"?>
<run>
    <tool>
        <title>Minus</title>
        <about>Subtract two values</about>
        <command>@tool/mainc @driver</command>
    </tool>
    <input>
        <integer id="value1">
            <about>
                <label>Value 1</label>
                <description>First value in equation</description>
            </about>
            <default>7</default>
        </integer>
        <integer id="value2">
            <about>
                <label>Value 2</label>
                <description>Second value in equation</description>
            </about>
            <default>3</default>
        </integer>
    </input>
    <output>
        <integer id="value3">
            <about>
                <label>Result</label>
                <description>result of subtraction</description>
            </about>
        </integer>
    </output>
</run>
